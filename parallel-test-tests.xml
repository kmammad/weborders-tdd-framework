<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Parallel Test Tests" parallel="tests" thread-count="30">

    <test name="Parallel Test Tests Example">
<classes>
     <class name="tests.LoginTests"/>
    <class name="tests.AllOrdersTests"/>
    <class name="tests.AllProductsTests"/>
</classes>
    </test>

    <test name="Parallel Test Methods Example">
        <classes>
            <class name="tests.LoginTests"/>
            <class name="tests.AllOrdersTests"/>
            <class name="tests.AllProductsTests"/>
        </classes>
    </test>

    <test name="Parallel Test Methods Example">
        <classes>
            <class name="tests.LoginTests"/>
            <class name="tests.AllOrdersTests"/>
            <class name="tests.AllProductsTests"/>
        </classes>
    </test>

</suite>